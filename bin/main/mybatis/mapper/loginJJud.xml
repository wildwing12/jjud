<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.jjudrestaurant.mapper.JjudMapper">
	<select id="loadUserByUsername" resultType="com.example.jjudrestaurant.VO.Memberjjud">
		select 
				*
		from memberjjud
		where EMAIL=#{email}
	</select>
</mapper>